  "angular-cli": {},
  "scripts": {
    "start": "ng build -w",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "postinstall": "cordova prepare",
    "start.cordova": "ng build && cordova serve",
    "start.windows": "ng build && cordova run windows",
    "install.windows": "cordova platform rm windows && cordova platform add windows --save",
    "start.ubuntu": "ng build && cordova run --device",
    "install.ubuntu": "cordova platform rm ubuntu && cordova platform add ubuntu --save",
    "start.ios": "ng build && cordova run ios",
    "install.ios": "cordova platform rm ios && cordova platform add ios --save",
    "start.osx": "ng build && cordova run",
    "install.osx": "cordova platform rm osx && cordova platform add osx --save",
    "start.android": "ng build && cordova run android",
    "install.android": "cordova platform rm android && cordova platform add android --save",
    "start.browser": "ng build && cordova run browser",
    "install.browser": "cordova platform rm browser && cordova platform add browser --save"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "2.1.1",

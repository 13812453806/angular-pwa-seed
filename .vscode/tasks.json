{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "args": [
    "run"
  ],
  "echoCommand": true,
  "tasks": [
    {
      "taskName": "start",
      "isBuildCommand": true,
      "showOutput": "always",
      "isBackground": true,
      "isTestCommand": false,
      "problemMatcher": {
        "owner": "angular-cli",
        "fileLocation": "relative",
        "base": "$tsc",
        "background": {
          "activeOnStart": true,
          "beginsPattern": "Angular Live Development Server",
          "endsPattern": "^Time:"
        }
      }
    },
    {
      "taskName": "test",
      "isBuildCommand": false,
      "showOutput": "always",
      "isBackground": true,
      "isTestCommand": true,
      "problemMatcher": "$tsc"
    },
    {
      "taskName": "lint",
      "command": "npm run lint",
      "isBackground": true,
      "problemMatcher": "$tsc"
    },
    {
      "taskName": "serve",
      "isBuildCommand": false,
      "showOutput": "always",
      "isBackground": true,
      "isTestCommand": false,
      "problemMatcher": {
        "owner": "angular-cli",
        "fileLocation": "relative",
        "base": "$tsc",
        "background": {
          "activeOnStart": true,
          "beginsPattern": "Angular Live Development Server",
          "endsPattern": "^Time:"
        }
      }
    }
  ]
}